{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.pyy"
    },
    {
      "src": "/(.*)",
      "dest": "package.json"
    }
  ],
  "functions": {
      "api/**": {
        "runtime": "python3.9",
        "excludeFiles": "{.next,commands,csv,notebooks,public,src}/**"
      }
  }
}

